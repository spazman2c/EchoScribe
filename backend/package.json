{
  "name": "echoscribe-backend",
  "version": "1.0.0",
  "description": "EchoScribe Node.js Express API backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
